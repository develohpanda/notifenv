{
  "name": "Notifenv",
  "description": "A simple browser action to add a color and header for the current environment.",
  "version": "0.1",
  "permissions":[
    "tabs",
    "debugger"
  ],
  "content_scripts":[
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2
}
